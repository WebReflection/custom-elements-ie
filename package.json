{
  "name": "@webreflection/custom-elements-ie",
  "version": "0.1.3",
  "description": "A better custom-elements-builtin polyfill, IE11+",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run copy:ce && npm run copy:bi && npm run cjs && npm run rollup:babel && npm run patch && npm run rollup:test && npm run min && npm run size",
    "copy:ce": "cp node_modules/@webreflection/custom-elements-no-builtin/esm/index.js esm/custom-elements.js",
    "copy:bi": "cp node_modules/@webreflection/custom-elements-builtin/esm/index.js esm/builtin.js",
    "cjs": "ascjs esm cjs",
    "patch": "sed -i.bck 's/new Promise/new Lie/; s/.*setPrototypeOf.*HTMLBuiltIn.*//' index.js && rm -rf index.js.bck",
    "rollup:babel": "rollup --config rollup/babel.config.js",
    "rollup:test": "rollup --config rollup/test.config.js",
    "min": "terser index.js --comments='/^!/' -c -m -o min.js",
    "size": "cat min.js | gzip | wc -c"
  },
  "keywords": [
    "customElements",
    "legacy",
    "Internet Explorer",
    "IE11"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "ascjs": "^4.0.1",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0",
    "terser": "^5.0.0"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "default": "./cjs/index.js"
  },
  "unpkg": "min.js",
  "dependencies": {
    "@webreflection/custom-elements-builtin": "^0.1.9",
    "@webreflection/custom-elements-no-builtin": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/custom-elements-ie.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/custom-elements-ie/issues"
  },
  "homepage": "https://github.com/WebReflection/custom-elements-ie#readme"
}
